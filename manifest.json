{
  "name": "Browser Pop",
  "version": "1.0",
  "description": "Custom reminders of urls.",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
